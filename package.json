{
  "name": "luckydegens-games",
  "version": "1.0.0",
  "license": "ISC",
  "scripts": {
    "start": "cross-env NODE_ENV=production pm2 start server.js",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "dev-inspect": "cross-env NODE_ENV=development nodemon --inspect=9230 server.js",
    "prettier": "prettier --write --config .prettierrc.json \"**/*.js\"",
    "lint": "eslint --fix --config .eslintrc.json \"**/*.js\"",
    "remark": "remark . -o"
  },
  "_husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run remark"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "airtable-plus": "^1.0.4",
    "axios": "^0.24.0",
    "babel-eslint": "^10.1.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "country-list": "^2.2.0",
    "date-fns": "^2.25.0",
    "dotenv-safe": "^8.2.0",
    "expeditious-engine-redis": "^0.1.2",
    "express": "^4.17.1",
    "express-expeditious": "^5.1.1",
    "express-validator": "^6.13.0",
    "form-data": "^4.0.0",
    "glob": "^7.2.0",
    "handlebars": "^4.7.7",
    "helmet": "^4.4.1",
    "ipdata": "^2.2.0",
    "jimp": "^0.16.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^6.0.13",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "request-country": "^0.1.5",
    "request-ip": "^2.1.3",
    "ua-parser-js": "^1.0.2",
    "uuid": "^8.3.2",
    "validator": "^13.7.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-formidable": "^4.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "prettier-eslint": "^12.0.0",
    "remark-cli": "^9.0.0"
  }
}
